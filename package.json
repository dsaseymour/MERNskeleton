{
  "name": "mernskeleton",
  "version": "1.0.0",
  "description": "perform basic functions common to MERN applications",
  "main": "./server/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "startdevserver": "cross-env NODE_ENV=dev nodemon server/index.js",
    "startprodserver": "cross-env NODE_ENV=prod nodemon server/index.js",
    "startdevclient": " cross-env NODE_ENV=dev nodemon npm start --prefix client",
    "startprodclient": "cross-env NODE_ENV=prod nodemon npm start --prefix client",
    "startalldev": "concurrently \"npm run startdevserver\" \"npm run startdevclient\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dsaseymour/MERNskeleton.git"
  },
  "keywords": [
    "MERN",
    "React"
  ],
  "author": "Danny Seymour",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dsaseymour/MERNskeleton/issues"
  },
  "homepage": "https://github.com/dsaseymour/MERNskeleton#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.4.0",
    "mongoose": "^5.4.6",
    "passport": "^0.4.0",
    "passport-facebook-token": "^3.3.0",
    "passport-google-plus-token": "^2.1.0",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "jest": "^23.6.0"
  }
}
